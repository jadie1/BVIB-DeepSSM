{
  "model_name": "BE_vib",
  "num_latent_dim": 10,
  "paths": {
    "out_dir": "SS_experiments/size_400/",
    "loader_dir": "SS_loaders/size_400/",
    "aug_dir": "SS_loaders/size_400/"
  },
  "encoder": {
    "stochastic": true,
    "covariance_type": "diagonal",
    "offset": false
  },
  "decoder": {
    "pca_intialized": false,
    "fixed": false,
    "linear": false
  },
  "loss": {
    "function": "vib_burnin",
    "supervised_latent": false,
    "params": {
      "initiate_stochastic": 20,
      "complete_stochastic": 40,
      "beta": 0.01
    }
  },
  "trainer": {
    "epochs": 2000,
    "learning_rate": 5e-05,
    "num_samples": 30,
    "decay_lr": {
      "enabled": false
    },
    "early_stop": {
      "enabled": true,
      "patience": 50,
      "start_epoch": 105
    },
    "weight_decay": 0,
    "gradient_clipping": true,
    "save_iter_freq": 0
  },
  "dropout": {
    "type": null,
    "params": {
      "start_epoch": 100
    }
  },
  "batch_ensemble": {
    "enabled": true,
    "mixup": false,
    "num_models": 4
  },
  "initialize_model": null,
  "use_best_model": true,
  "size": 400
}